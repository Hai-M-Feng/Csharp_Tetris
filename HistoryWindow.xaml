<Window x:Class="俄罗斯方块promax.HistoryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:俄罗斯方块promax"
        mc:Ignorable="d"
        Title="HistoryWindow" Height="700" Width="400" Loaded="Window_Loaded">
    <Grid>
        <DataGrid Margin="10" x:Name="dataGrid" AutoGenerateColumns="False" CanUserAddRows="True">
            <DataGrid.Columns>
                <DataGridTextColumn Header="分数" Binding="{Binding Column1}" Width="0.5*"/>
                <DataGridTextColumn Header="开始时间" Binding="{Binding Column2}" Width="*"/>
                <DataGridTextColumn Header="结束时间" Binding="{Binding Column3}" Width="*"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
